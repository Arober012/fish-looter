{
  "name": "obs-overlay-fishing-game",
  "version": "1.0.0",
  "description": "An interactive fishing game overlay for OBS controlled via Twitch chat commands.",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "start:prod": "node dist/server/index.js",
    "build": "tsc",
    "dev": "ts-node-dev src/server/index.ts",
    "overlay:dev": "vite",
    "overlay:build": "cross-env VITE_TARGET=overlay VITE_OUTDIR=dist/overlay vite build",
    "overlay:preview": "vite preview",
    "panel:build": "cross-env VITE_TARGET=panel VITE_OUTDIR=dist/panel vite build"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tmi.js": "^1.8.5",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/tmi.js": "^1.8.6",
    "@vitejs/plugin-react": "^5.1.2",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "vite": "^5.4.21"
  },
  "keywords": [
    "OBS",
    "fishing",
    "game",
    "Twitch",
    "overlay"
  ],
  "author": "Your Name",
  "license": "MIT"
}
